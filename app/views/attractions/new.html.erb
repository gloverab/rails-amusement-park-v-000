<h1>New Attraction</h1>
  <%= form_for :attraction do |f| %>
    Name: <%= f.text_field :name %>
    Tickets Needed: <%= f.number_field :tickets %>
    Nausea Rating: <%= f.number_field :nausea_rating %>
    Happiness Rating: <%= f.number_field :happiness_rating %>
    Height Requirement <%= f.number_field :min_height %>
  <%= f.submit "Create Attraction" %>
<% end %>
